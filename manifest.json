{
  "manifest_version": 3,
  "name": "Auto Form Filler",
  "version": "1.0",
  "description": "Fill forms automatically with your LinkedIn data and custom fields.",

  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "index.html"
  }
}